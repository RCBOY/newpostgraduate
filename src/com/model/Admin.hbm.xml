<?xml version='1.0' encoding='utf-8'?>
          <!DOCTYPE hibernate-mapping
        PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 <hibernate-mapping >
<class name="com.model.Admin" table="tb_admin" schema="newpostgraduate2">
    <id name="id" type="java.lang.Integer" column="aid">
        <generator class="native"></generator>
    </id>
    <property name="acc_anum" type="java.lang.String" >
        <column name="acc_anum" />
    </property>
    <property name="name" type="java.lang.String" >
        <column name="aname" />
    </property>
    <property name="apassword" type="java.lang.String" >
        <column name="apassword" />
    </property>
    <property name="tel" type="java.lang.String" >
        <column name="atel"/>
    </property>
</class>
</hibernate-mapping>